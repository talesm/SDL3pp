{
  "include": ["scripts/**/*.ts"],
  "exclude": ["scripts/**/dist/"],
  "compilerOptions": {
    "lib": [
      "ES2024"
    ],
    "resolveJsonModule": true,
    "checkJs": true,
    "module": "node16",
    "outDir": "dist/",
    "target": "es2020",
    "moduleResolution": "node16",
    "declaration": false,
    "noEmit": true
  },
}
